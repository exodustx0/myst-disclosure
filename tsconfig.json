{
	"include": ["src/**/*.ts"],
	"compilerOptions": {
		"outDir": "dist",
		"module": "ES2020",
		"target": "ES2019",
		"lib": ["ES2020"],
		"moduleResolution": "node",
		"strict": true,
		"importHelpers": true,
		"esModuleInterop": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noEmitOnError": true,
		"resolveJsonModule": false,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"importsNotUsedAsValues": "error"
	}
}
